#include<stdio.h>

int main(void){
	int A,B,i;
	int M =0,T=0;
	
	printf("indica el valor de A-:");
	scanf("%d", &A);
	printf("indica el valor de B-:");
	scanf("%d", &B);
	
	if(A>B) M=A;
    else M=B;
	
	for(i=1;  i<=M; i++)
	{
		T=T+i;
		printf("%d+",i);
	}
	printf("sumatoria total: %d",T);
	return 0;
}
