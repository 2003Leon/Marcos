#include <stdio.h>
#include <math.h>

int main(void){
	
	float datos[13]={3.3, 4.5, 5.0, 3.4, 4.2, 5.3, 3.2, 4.3, 5.2, 7.1, 3.3, 4.8, 5.2};
	int jkl;
	if(3.5>datos[jkl]){
	printf("\nImperceptible");}
	else{
	printf("\nSin problemas");}
	if (3.5<datos[jkl]<5.4 ){
    printf("\nSin problemas");}
	else{
	printf("\nAvisar");}
	if(5.5<datos[jkl]<6.0){
    printf("\nEmitir alerta");}
	else{
	printf("\nDesalojar");}
	if(6.1<datos[jkl]<6.9){	
	printf("\nEmitir alerta");}
	else{
	printf("\nEnviar equipos de apoyo");}
	if(7.0<datos[jkl]<7.9){
	printf("\nEmitir alerta");}
	else{
	printf("\nEnviar equipos de rescate");}
	if(datos[jkl]>8.0){	
	printf("\nEmergencia nacional");}
	else{
	printf("\nSolicitar apoyo externo");
	}return 0;}
